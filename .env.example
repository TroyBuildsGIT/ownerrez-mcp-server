# OwnerRez API Configuration
# Copy this file to .env and fill in your actual values

OWNERREZ_OAUTH_CLIENT_ID=your_oauth_client_id_here
OWNERREZ_EMAIL=your_email@example.com
OWNERREZ_PASSWORD=your_password_here
OWNERREZ_API_KEY=your_api_key_here
OWNERREZ_API_BASE_URL=https://app.ownerrez.com/api
OWNERREZ_API_DOCS_URL=https://www.ownerrez.com/support/articles/api-oauth-app
